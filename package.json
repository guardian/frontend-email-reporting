{
  "name": "emailReporting",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "bowerJs": "cd assets/javascripts && rimraf lib/bower-components &&  bower install",
    "bowerCss": "cd assets/stylesheets && rimraf bower_components &&  bower install",
    "bowerSetup": "npm run bowerJs && npm run bowerCss",
    "devSetup": "npm run compile --dev",
    "compile": "echo 'compiling assets' && grunt compile",
    "compileCss": "grunt compile:css --dev",
    "compileJs": "grunt compile:js --dev",
    "watch": "grunt watch --dev",
    "test": "grunt validate && grunt karma",
    "postinstall": "npm run bowerSetup",
    "cloudformation": "yaml2json cloudformation/cloudformation.template.yaml"
  },
  "devDependencies": {
    "bower": "^1.5.3",
    "grunt": "^1.0.1",
    "grunt-asset-hash": "^0.1.6",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-requirejs": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^17.2.0",
    "grunt-karma": "^2.0.0",
    "grunt-sass": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-requirejs": "~0.2.2",
    "load-grunt-tasks": "^3.2.0",
    "mkdirp": "^0.5.1",
    "phantomjs": "~1.9.17",
    "requirejs": "^2.1.18",
    "rimraf": "^2.4.3",
    "time-grunt": "^1.2.1",
    "yamljs": "^0.2.8"
  }
}
